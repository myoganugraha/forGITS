<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="itk.myoganugraha.soalgits">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Login">
            <intent-filter
                android:label="@string/app_name">
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Register"
            android:label="Register" />
        <activity
            android:name=".MainActivity"
            android:label="Home" />
        <activity
            android:name=".Posting"
            android:label="Tambahkan Informasi Wisata"/>
        <activity
            android:name=".DataranTinggi"
            android:label="Dataran Tinggi"
            android:parentActivityName=".MainActivity"/>
        <activity
            android:name=".DataranRendah"
            android:label="Dataran Rendah"
            android:parentActivityName=".MainActivity"
            />
        <activity
            android:name=".Pantai"
            android:label="Pantai"
            android:parentActivityName=".MainActivity"/>
    </application>

</manifest>